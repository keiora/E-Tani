<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.e_tani.ui.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/dashboard" />

    <fragment
        android:id="@+id/navigation_statistic"
        android:name="com.example.e_tani.ui.statistic.StatisticFragment"
        android:label="Account"
        tools:layout="@layout/statistic" />


    <fragment
        android:id="@+id/navigation_form"
        android:name="com.example.e_tani.ui.form.FormFragment"
        android:label="Form"
        tools:layout="@layout/form" />

</navigation>
